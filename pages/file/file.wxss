page{
  background-color: #f5f5f5;
}
.no-login{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 350rpx;
}
.no-login-image{
  width:50px;
  height:50px;
}
.no-login-font{
  margin-top: 20rpx;
}
.no-login-button{
  width: 400rpx;
  margin-top: 40rpx;
  background-color: #FF9933;
  border-radius: 50rpx;
}
/* 盒子 */
.main{
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20px;
  /* justify-content: space-between; */
}
.main-box{
  height: 300rpx;
  margin-top: 30rpx;
  background-color: white;
  border-radius: 24rpx;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.main-box-image{
  width: 100%;
  height: 75%;
}
.main-box-text{
  color: #a7a7a7;
  font-size: 28rpx;
  margin-top: 16rpx;
  margin-left: 22rpx;
}
.main-file {
  height: 100%;
}
.load-data{
  width: 100%;
  height: 35px;
  line-height: 40px;
  text-align: center;
  color: #8d8d8d;
  font-size: 30rpx;
  padding-bottom: 20px;
}